{
  "name": "tree-hooks",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/app/index.js",
      "default": "./dist/app/index.js"
    }
  },
  "scripts": {
    "format": "prettier --write *.{js,cjs,mjs,ts,cts,mts} {src,test*}/**/*.ts",
    "lint": "eslint {src,test*}/**/*.{js,ts}",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "prebuild": "npm run format && npm run lint:fix && npm test && rimraf ./dist",
    "tsc": "tsc -p tsconfig.app.json",
    "tsc-alias": "tsc-alias -p tsconfig.app.json",
    "build": "npm run tsc && npm run tsc-alias",
    "prepack": "npm run build",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Lex Borisoff <lex@borisoff.dev>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LexBorisoff/fs-hooks.git"
  },
  "homepage": "https://github.com/LexBorisoff/fs-hooks/tree/main#readme",
  "bugs": {
    "url": "https://github.com/LexBorisoff/fs-hooks/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@lexjs/eslint-plugin": "^1.0.0",
    "@types/node": "^22.3.0",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.15.0",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.1.1"
  }
}
